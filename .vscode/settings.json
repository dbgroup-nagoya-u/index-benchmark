{
  "editor.rulers": [
    88,
    100
  ],
  "editor.renderWhitespace": "all",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/build": true,
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true,
  },
  "terminal.integrated.env.linux": {
    "NUM_THREADS": "1",
  },
  "testMate.cpp.test.runtimeLimit": 60,
  "testMate.cpp.test.executables": "build/**/test/*_test",
  "[cmake]": {
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
  },
  "cmake.configureOnOpen": true,
  "cmake.buildDirectory": "${workspaceRoot}/build/${buildType}",
  "cpplint.lineLength": 100,
  "cpplint.filters": [
    "-build/c++11",
    "-readability/fn_size",
    "-runtime/references",
    "-whitespace/braces",
    "-whitespace/newline",
  ],
  "cSpell.words": [
    "CPUs",
    "DBUILD",
    "DCMAKE",
    "DHOMEGROWN",
    "DINDEX",
    "DMWCAS",
    "GCID",
    "GLOG",
    "JEMALLOC",
    "MIMALLOC",
    "NUMS",
    "PMEM",
    "PMWCAS",
    "Wextra",
    "alignas",
    "bwtree",
    "bztree",
    "ctest",
    "gflags",
    "gtest",
    "istr",
    "kvepoch",
    "kvio",
    "kvtimestamp",
    "lcdf",
    "libgflags",
    "libjson",
    "libnuma",
    "mrcu",
    "mwcas",
    "nlohmann",
    "numactl",
    "pbbslib",
    "pthread",
    "ptree",
    "stoul",
    "wangziqi",
    "zipf"
  ],
  "files.associations": {
    "fstream": "cpp",
    "thread": "cpp",
    "*.tcc": "cpp",
    "memory_resource": "cpp",
    "sstream": "cpp",
    "algorithm": "cpp",
