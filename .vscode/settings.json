{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/build": true,
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true,
  },
  "testMate.cpp.test.runtimeLimit": 60,
  "testMate.cpp.test.executables": "build/**/test/*_test",
  "[cmake]": {
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
  },
  "cpplint.lineLength": 100,
  "cpplint.filters": [
    "-readability/fn_size",
    "-whitespace/braces",
    "-build/c++11",
    "-whitespace/newline",
    "-runtime/references"
  ],
  "cSpell.words": [
    "CPUs",
    "DBUILD",
    "DCMAKE",
    "DMWCAS",
    "GLOG",
    "NUMS",
    "PMEM",
    "PMWCAS",
    "Wextra",
    "alignas",
    "ctest",
    "gflags",
    "gtest",
    "libgflags",
    "libnuma",
    "mwcas",
    "numactl",
    "pthread"
  ],
  "files.associations": {
    "fstream": "cpp",
    "thread": "cpp",
    "*.tcc": "cpp",
    "memory_resource": "cpp",
    "sstream": "cpp",
    "algorithm": "cpp",
    "array": "cpp",
    "atomic": "cpp",
    "cassert": "cpp",
    "cctype": "cpp",
    "cerrno": "cpp",
    "cfloat": "cpp",
    "chrono": "cpp",
    "climits": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "codecvt": "cpp",
    "condition_variable": "cpp",
    "cstdarg": "cpp",
    "cstddef": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "deque": "cpp",
    "forward_list": "cpp",
    "list": "cpp",
    "map": "cpp",
    "set": "cpp",
    "string": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "vector": "cpp",
    "exception": "cpp",
    "functional": "cpp",
    "iterator": "cpp",
    "memory": "cpp",
    "numeric": "cpp",
    "optional": "cpp",
    "random": "cpp",
    "ratio": "cpp",
    "string_view": "cpp",
    "system_error": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "utility": "cpp",
    "hash_map": "cpp",
    "hash_set": "cpp",
    "slist": "cpp",
    "initializer_list": "cpp",
    "iomanip": "cpp",
    "ios": "cpp",
    "iosfwd": "cpp",
    "iostream": "cpp",
    "istream": "cpp",
    "limits": "cpp",
    "locale": "cpp",
    "mutex": "cpp",
    "new": "cpp",
    "ostream": "cpp",
    "queue": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "cinttypes": "cpp",
    "typeinfo": "cpp",
    "bit": "cpp",
    "any": "cpp",
    "cstring": "cpp",
    "future": "cpp",
    "variant": "cpp",
    "shared_mutex": "cpp"
  },
}